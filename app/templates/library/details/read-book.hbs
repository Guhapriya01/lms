{{page-title "ReadBook"}}

<div align="center">
    <h3>{{this.model.attributes.name}}</h3>

    <label for="name">Name</label>
    <Input id="name" @value={{this.name}} @type="text" /><br><br>

    <label for="email">Email : </label>
    <Input id="email" @value={{this.email}} @type="text" /><br><br>

    <button type="button" {{on 'click' this.getBook}}>Get Book</button>
</div>